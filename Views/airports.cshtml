<h1> Airport Planner </h1>
<hr />
<h2>Airports:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var airport in Model)
    {
      <li><a href="/airports/@airport.GetId()">@airport.GetName()</a></li>
    }
  </ul>
}

<h4><a href = "/airports/new"> Add new airport</a></h4>
<h4><a href = "/"> Return home </a></h4>
